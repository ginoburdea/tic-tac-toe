{
    "name": "tic-tac-toe",
    "private": true,
    "version": "1.0.1",
    "type": "module",
    "scripts": {
        "dev": "concurrently --kill-others --names emulator,frontend --prefix-colors blue,green \"firebase emulators:start\" \"vite --host\"",
        "build": "dotenv-cli vite build",
        "preview": "concurrently --kill-others --names emulator,frontend --prefix-colors blue,green \"firebase emulators:start\" \"vite preview\"",
        "lint": "eslint **/*.{js,jsx} --fix",
        "format": "prettier --write --ignore-unknown .",
        "prepare": "husky install"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.2",
        "firebase": "^9.15.0",
        "nanoid": "^4.0.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.6.0",
        "tic-tac-toe-common": "github:ginoburdea/tic-tac-toe-common",
        "vite-plugin-svgr": "^2.4.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@vitejs/plugin-react": "^3.0.0",
        "concurrently": "^7.6.0",
        "dotenv-cli": "^7.0.0",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.31.11",
        "firebase-tools": "^11.19.0",
        "husky": "^8.0.2",
        "lint-staged": "^13.1.0",
        "prettier": "^2.8.4",
        "prettier-plugin-organize-imports": "^3.2.2",
        "sass": "^1.57.1",
        "vite": "^4.0.0"
    }
}
